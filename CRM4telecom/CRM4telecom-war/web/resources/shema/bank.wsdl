      </input>
      <output>
        <soap:body use="literal"/>
      </output>
    </operation>
  </binding>
  <service name="services">
    <port name="BillingPort" binding="tns:BillingPortBinding">
      <soap:address location="http://wawiloncomp:8088/mockBillingPortBinding"/>
    </port>
  </service>
